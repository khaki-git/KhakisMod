    "undefined-global",
    "duplicate-set-field",
    "unused-local",
    "inject-field"
    ],
    "globals": [
    "to_big",
    "to_number",
    "Event",
    "create_UIBox_your_collection_stickers",
    "create_UIBox_Other_GameObjects",
    "hand_chips",
