{
    "format": {
    "defaultConfig": {
    "align_continuous_assign_statement": "false"
    }
    },
    "workspace": {
    "library": [
    "../smods/",
    "../../love_api",
    "../lovely/dump"
    ]
    },
    "runtime": {
    "version": "LuaJIT"
    },
    "codeLens": {
